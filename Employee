public class Employee{
	static int emps;
        private Employee(int emps)
	{
        	emps = emps;
        	System.out.println(emps + " employee created.");
               }
       	public static void main(String args[]) 
	{
        		Employee e1 = new Employee(1);
        		Employee e2 = new Employee(2);
        		Employee e3 = new Employee(3);
      	  }
}
